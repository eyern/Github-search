    "@progress/kendo-angular-buttons": "^5.0.0",
    "@progress/kendo-angular-charts": "^4.0.0",
    "@progress/kendo-angular-intl": "^2.0.0",
    "@progress/kendo-angular-l10n": "^2.0.0",
    "@progress/kendo-angular-layout": "^4.0.0",
    "@progress/kendo-drawing": "^1.5.7",
    "@progress/kendo-theme-default": "^4.0.0",
    "bootstrap": "^4.1.3",
    "core-js": "3.1.4",
    "hammerjs": "^2.0.8",
    "ngrx-store-freeze": "~0.2.3",
    "ngrx-store-logger": "~0.2.1",
    "rxjs": "~6.5.2",
    "socket.io": "^2.1.1",
    "web-animations-js": "^2.3.1",
    "zone.js": "~0.9.1"
  },
